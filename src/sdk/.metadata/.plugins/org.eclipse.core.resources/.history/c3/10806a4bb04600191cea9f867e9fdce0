#include "peripheral.h"
#include "xgpio.h"
#include <stdio.h>

XGpio gpio;
double angle;

void timerCallback()
{
//	xil_printf("timer tick!");
	XGpio_DiscreteWrite(&gpio, 1, );
}

int main(void)
{
	timerSetup(&timerCallback);

	XGpio_Initialize(&gpio, 1);
	XGpio_SetDataDirection(&gpio, 1, 0x00000000);

	angle = 0.0;

	for(;;);
}
